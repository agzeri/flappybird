{
  "name": "flappybird",
  "version": "1.0.0",
  "description": "The famous game now on the web.",
  "main": "index.js",
  "private": true,
  "scripts": {
    "precommit": "npm t",
    "predocs": "rimraf docs",
    "docs": "jsdoc -t template -c jsdoc.json -R README.md -r",
    "lint:js": "eslint tests/**/*.js",
    "lint:sass": "sass-lint --config .scss-lint.yml --verbose --no-exit",
    "pretest": "npm run lint:js && npm run lint:sass",
    "test": "mocha tests/** --compilers js:babel-core/register"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ro9ues/flappybird.git"
  },
  "keywords": [
    "FlappyBird",
    "ESNEXT",
    "TDD",
    "Webpack",
    "JavaScript"
  ],
  "author": "@agzeri",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ro9ues/flappybird/issues"
  },
  "homepage": "https://github.com/ro9ues/flappybird#readme",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "eslint": "^4.5.0",
    "husky": "^0.14.3",
    "jsdoc": "^3.5.4",
    "mocha": "^3.5.0",
    "node-sass": "^4.5.3",
    "rimraf": "^2.6.1",
    "sass-lint": "^1.10.2",
    "sass-true": "^3.0.0"
  }
}
